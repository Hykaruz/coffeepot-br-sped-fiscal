<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
    <stream name="sped" format="delimited">
        <parser>
            <property name="delimiter" value="|" />
            <property name="recordTerminator" value="\r\n" />      
        </parser>
        
        <typeHandler type="string" class="coffeepot.br.sped.fiscal.typeHandler.StringHandler" />
        <typeHandler type="date" class="coffeepot.br.sped.fiscal.typeHandler.DateHandler" />
        <typeHandler type="double" class="coffeepot.br.sped.fiscal.typeHandler.FloatHandler" />
        <typeHandler name="tipoItemHandler" class="coffeepot.br.sped.fiscal.typeHandler.TipoItemHandler" />
        
        <group name="reg0200" minOccurs="0" maxOccurs="unbounded" class="coffeepot.br.sped.fiscal.blocos.bloco0.Reg0200" >                        
            <record name="reg0200" order="1" minOccurs="1">
                <field name="delimiter" literal="" ignore="true"/>
                <field name="reg" rid="true" literal="0200" ignore="true" />
                <field name="codItem" type="string"  />
                <field name="descrItem" type="string"  />
                <field name="codBarra" type="string" />
                <field name="codAntItem" type="string"  />
                <field name="unidInv" type="string" />
                <field name="tipoItem" typeHandler="tipoItemHandler" length="2" justify="right" padding="0"/>
                <field name="codNcm" type="string" format="numbersOnly=true; min=8" />
                <field name="exIpi" type="string" />
                <field name="codGen" type="string" format="min=2; max=2; justify=right; padding=0"/>
                <field name="codLst" type="int"/>
                <field name="aliqIcms" type="double" />
                <field name="delimiter" literal="" ignore="true"/>
            </record>
            <record name="reg0205List" class="coffeepot.br.sped.fiscal.blocos.bloco0.Reg0205" collection="list" order="2" minOccurs="0" maxOccurs="unbounded">
                <field name="delimiter" literal="" ignore="true"/>
                <field name="reg" rid="true" literal="0205" ignore="true" />
                <field name="descrAntItem" type="string"/>
                <field name="dtIni" type="date" />
                <field name="dtFim" type="date" />
                <field name="codAntItem" type="string" />
                <field name="delimiter" literal="" ignore="true"/>
            </record>
            <record name="reg0206" class="coffeepot.br.sped.fiscal.blocos.bloco0.Reg0206" order="3" minOccurs="0" maxOccurs="1">
                <field name="delimiter" literal="" ignore="true"/>
                <field name="reg" rid="true" literal="0206" ignore="true" />
                <field name="codComb" type="string"/>
                <field name="delimiter" literal="" ignore="true"/>
            </record>
            <record name="reg0220List" class="coffeepot.br.sped.fiscal.blocos.bloco0.Reg0220" collection="list" order="4" minOccurs="0" maxOccurs="unbounded">
                <field name="delimiter" literal="" ignore="true"/>
                <field name="reg" rid="true" literal="0220" ignore="true" />
                <field name="unidConv" type="string"/>
                <field name="fatConv" type="double" />            
                <field name="delimiter" literal="" ignore="true"/>
            </record>
        </group>
    </stream>
</beanio>